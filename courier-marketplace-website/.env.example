# Application Configuration
REACT_APP_NAME=Exalt Courier Marketplace
REACT_APP_VERSION=$npm_package_version
REACT_APP_ENVIRONMENT=development
REACT_APP_DEBUG=true

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8080/api/v1
REACT_APP_COURIER_MANAGEMENT_API_URL=http://localhost:8081/api/v1
REACT_APP_TRACKING_SERVICE_URL=http://localhost:8082/api/v1
REACT_APP_FARE_CALCULATOR_URL=http://localhost:8083/api/v1
REACT_APP_COURIER_ONBOARDING_URL=http://localhost:8084/api/v1
REACT_APP_CUSTOMER_ONBOARDING_URL=http://localhost:8085/api/v1
REACT_APP_GEO_ROUTING_URL=http://localhost:8086/api/v1
REACT_APP_PICKUP_ENGINE_URL=http://localhost:8087/api/v1
REACT_APP_LOCATION_TRACKER_URL=http://localhost:8088/api/v1
REACT_APP_NOTIFICATION_SERVICE_URL=http://localhost:8089/api/v1

# WebSocket Configuration
REACT_APP_WEBSOCKET_URL=ws://localhost:8080/ws
REACT_APP_TRACKING_WEBSOCKET_URL=ws://localhost:8082/tracking
REACT_APP_ENABLE_WEBSOCKET=true
REACT_APP_ENABLE_REAL_TIME_TRACKING=true

# Authentication
REACT_APP_AUTH_DOMAIN=auth.couriermarketplace.com
REACT_APP_AUTH_CLIENT_ID=courier-marketplace-web-client
REACT_APP_AUTH_REDIRECT_URI=http://localhost:3002/auth/callback
REACT_APP_AUTH_SCOPE=openid profile email
REACT_APP_TOKEN_STORAGE_KEY=exalt_courier_token
REACT_APP_REFRESH_TOKEN_KEY=exalt_courier_refresh_token

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_SOCIAL_LOGIN=true
REACT_APP_ENABLE_GEOLOCATION=true
REACT_APP_ENABLE_LIVE_TRACKING=true
REACT_APP_ENABLE_ROUTE_OPTIMIZATION=true
REACT_APP_ENABLE_COURIER_RATINGS=true
REACT_APP_ENABLE_INSTANT_QUOTES=true
REACT_APP_ENABLE_SCHEDULED_PICKUPS=true
REACT_APP_ENABLE_MULTI_PACKAGE=true
REACT_APP_ENABLE_INTERNATIONAL_SHIPPING=true
REACT_APP_ENABLE_BULK_SHIPPING=true
REACT_APP_ENABLE_WHITE_GLOVE_SERVICE=true

# UI Configuration
REACT_APP_DEFAULT_LANGUAGE=en
REACT_APP_SUPPORTED_LANGUAGES=en,es,fr,de,zh,ar
REACT_APP_DEFAULT_CURRENCY=USD
REACT_APP_SUPPORTED_CURRENCIES=USD,EUR,GBP,CAD,AUD
REACT_APP_DATE_FORMAT=MM/DD/YYYY
REACT_APP_TIME_FORMAT=HH:mm:ss

# Search Configuration
REACT_APP_DEFAULT_SEARCH_RADIUS=25
REACT_APP_MAX_SEARCH_RADIUS=100
REACT_APP_DEFAULT_PAGE_SIZE=20
REACT_APP_MAX_PAGE_SIZE=100

# Geolocation & Maps
REACT_APP_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
REACT_APP_MAPBOX_API_KEY=your-mapbox-api-key
REACT_APP_DEFAULT_LATITUDE=40.7128
REACT_APP_DEFAULT_LONGITUDE=-74.0060
REACT_APP_DEFAULT_ZOOM=12
REACT_APP_ENABLE_GPS_LOCATION=true

# Courier Service Configuration
REACT_APP_SERVICE_TYPES=standard,express,overnight,same_day,white_glove,international
REACT_APP_PACKAGE_TYPES=document,small_package,medium_package,large_package,fragile,food,pharmaceutical
REACT_APP_DELIVERY_SPEEDS=standard,express,rush,scheduled
REACT_APP_MIN_PACKAGE_WEIGHT=0.1
REACT_APP_MAX_PACKAGE_WEIGHT=150
REACT_APP_WEIGHT_UNITS=kg,lbs

# Distance & Routing
REACT_APP_DISTANCE_UNITS=miles,kilometers
REACT_APP_MAX_DELIVERY_DISTANCE=500
REACT_APP_ENABLE_ROUTE_PREVIEW=true
REACT_APP_ENABLE_TRAFFIC_ESTIMATION=true

# File Upload
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,image/webp,application/pdf

# Payment Configuration
REACT_APP_STRIPE_PUBLIC_KEY=pk_test_xxxxxxxxxxxxx
REACT_APP_PAYPAL_CLIENT_ID=your-paypal-client-id
REACT_APP_ENABLE_CREDIT_PAYMENTS=true
REACT_APP_ENABLE_COD_PAYMENTS=true
REACT_APP_ENABLE_CORPORATE_BILLING=true

# Courier Features
REACT_APP_COURIER_VEHICLE_TYPES=bike,motorcycle,car,van,truck
REACT_APP_ENABLE_COURIER_PREFERENCES=true
REACT_APP_ENABLE_DELIVERY_PREFERENCES=true
REACT_APP_ENABLE_SPECIAL_INSTRUCTIONS=true
REACT_APP_ENABLE_PROOF_OF_DELIVERY=true
REACT_APP_ENABLE_SIGNATURE_REQUIRED=true

# Analytics
REACT_APP_GA_TRACKING_ID=UA-XXXXXXXXX-X
REACT_APP_GTM_ID=GTM-XXXXXXX
REACT_APP_MIXPANEL_TOKEN=your-mixpanel-token
REACT_APP_HOTJAR_ID=your-hotjar-id

# Social Media
REACT_APP_FACEBOOK_APP_ID=your-facebook-app-id
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id

# CDN Configuration
REACT_APP_CDN_URL=https://cdn.couriermarketplace.com
REACT_APP_IMAGE_CDN_URL=https://images.couriermarketplace.com
REACT_APP_COURIER_IMAGES_CDN=https://courier-images.couriermarketplace.com

# Error Tracking
REACT_APP_SENTRY_DSN=https://xxxxxxxxx@sentry.io/xxxxxxx

# Performance Monitoring
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_PERFORMANCE_SAMPLE_RATE=0.1

# Security
REACT_APP_ENABLE_CSP=true
REACT_APP_CSP_REPORT_URI=https://csp.couriermarketplace.com/report

# Cache Configuration
REACT_APP_CACHE_TTL=3600
REACT_APP_ENABLE_SERVICE_WORKER=true
REACT_APP_COURIER_CACHE_TTL=1800
REACT_APP_TRACKING_CACHE_TTL=300

# Development Tools
REACT_APP_ENABLE_REDUX_DEVTOOLS=true
REACT_APP_ENABLE_REACT_DEVTOOLS=true

# Business Configuration
REACT_APP_COMPANY_NAME=Exalt Courier Services
REACT_APP_COMPANY_PHONE=1-800-COURIER
REACT_APP_COMPANY_EMAIL=support@couriermarketplace.com
REACT_APP_SUPPORT_HOURS=24/7
REACT_APP_EMERGENCY_CONTACT=1-800-EMERGENCY

# Operating Hours
REACT_APP_DEFAULT_OPERATING_HOURS_START=06:00
REACT_APP_DEFAULT_OPERATING_HOURS_END=22:00
REACT_APP_ENABLE_24_7_SERVICE=true

# Special Services
REACT_APP_ENABLE_TEMPERATURE_CONTROLLED=true
REACT_APP_ENABLE_HAZMAT_SHIPPING=false
REACT_APP_ENABLE_PRESCRIPTION_DELIVERY=true
REACT_APP_ENABLE_ALCOHOL_DELIVERY=true
REACT_APP_ENABLE_FURNITURE_DELIVERY=true